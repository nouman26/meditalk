																				<!-- <a href="/appointment/accept/<%= pat.email%>-<%= data.name%>" class="btn btn-sm bg-success-light">
																					<i class="fas fa-check"></i> Accept
																				</a>
																				<a href="/appointment/cancel/<%= pat.email%>-<%= data.name%>" class="btn btn-sm bg-danger-light">
																					<i class="fas fa-times"></i> Cancel
																				</a> -->
																			</div>
																		</td>
																	</tr>
																	<% })%>
