											<!--Appointment Tab -->
											<div class="tab-pane show active" id="upcoming-appointments">
												<div class="card card-table mb-0">
													<div class="card-body">
														<div class="table-responsive">
															<table class="table table-hover table-center mb-0">
																<thead>
																	<tr>
																		<th>Patient Name</th>
																		<th>Date</th>
																		<th>time</th>
																		<th></th>
																		<th></th>
																		<th></th>
																	</tr>
																</thead>
																<tbody>
																	<%datapat.forEach(function(pat){ %>
																	<tr>
																		<td>
																			<h2 class="table-avatar">
																				<a href="patient-profile.html" class="avatar avatar-sm mr-2"><img class="avatar-img rounded-circle" src="/user_images/<%= pat.patient_image%>" alt="User Image"></a>
																				<a href="patient-profile.html"><%= pat.name%> <span><%= pat.email%></span></a>
																			</h2>
																		</td>
																		<td><%= pat.date%></td>
																		<td><%= pat.time%></td>
																		<td class="text-right">
																			<div class="table-action">
																				<td><a href="/appointment/view_details/<%= pat.email%>" class="btn btn-sm bg-info-light">
																					<i class="far fa-eye"></i> View
																				</a></td>
																				<td><form action="/appointment/accept" method="POST">
																					<input type="hidden" name=name value="<%= pat.name%>">
																					<input type="hidden" name=email value="<%= pat.email%>">
																					<input type="hidden" name=patient_image value="<%= pat.patient_image%>">
																					<input type="hidden" name=ac_type value="<%= pat.ac_type%>">
																					<input type="hidden" name=account_no value="<%= pat.account_no%>">
																					<input type="hidden" name=date value="<%= pat.date%>">
																					<input type="hidden" name=time value="<%= pat.time%>">
																					<input type="hidden" name=trx value="<%= pat.trx%>">
																					<input type="hidden" name=image value="<%= pat.image%>">
																					<input type="hidden" name=condition value="accept">
																					<input type="hidden" name=doctor value="<%= data.name%>">
																					<input type="hidden" name=doctor_image value="<%= data.image%>">
																					<button style="background: green; color: white;"><i class="fas fa-check"></i> Accept</button>
																				</form></td>
																				<td><form action="/appointment/cancel" method="POST">
																					<input type="hidden" name=name value="<%= pat.name%>">
																					<input type="hidden" name=email value="<%= pat.email%>">
																					<input type="hidden" name=patient_image value="<%= pat.patient_image%>">
																					<input type="hidden" name=ac_type value="<%= pat.ac_type%>">
																					<input type="hidden" name=account_no value="<%= pat.account_no%>">
																					<input type="hidden" name=date value="<%= pat.date%>">
																					<input type="hidden" name=time value="<%= pat.time%>">
																					<input type="hidden" name=trx value="<%= pat.trx%>">
																					<input type="hidden" name=image value="<%= pat.image%>">
																					<input type="hidden" name=condition value="accept">
																					<input type="hidden" name=doctor value="<%= data.name%>">
																					<input type="hidden" name=doctor_image value="<%= data.image%>">
																					<button style="background: red;color: white;"><i class="fas fa-times"></i> Cancel</button>
																				</form ></td>
																				<!-- <a href="/appointment/view_details/<%= pat.email%>" class="btn btn-sm bg-info-light">
																					<i class="far fa-eye"></i> View
